declare tab[10];
tab[0] = 9;
tab[1] = 54;
tab[2] = 7;
tab[3] = 37;
tab[4] = 5;
tab[5] = 2;
tab[6] = 9;
tab[7] = 2;
tab[8] = 1;
tab[9] = 12;
taille_tab = 10;
tri_bulle()
{
    local i = 0;
    local j = 0;
    while test ${i} -lt $(expr $1 - 1) do
        j = 0;
        while test ${j} -lt $(expr $1 - ${i} - 1) do
            if test ${tab[${j}]} -gt ${tab[(${j} + 1)]} then
                tmp1 = ${tab[${j}]};
                tmp2 = ${tab[(${j} + 1)]};
                tab[${j}] = ${tmp2};
                tab[(${j} + 1)] = ${tmp1}
            fi;
            j = $(expr ${j} + 1)
        done;
        i = $(expr ${i} + 1)
    done;
    return
};
tri_bulle ${taille_tab};
echo ${tab[0]} ${tab[1]} ${tab[2]} ${tab[3]} ${tab[4]} ${tab[5]} ${tab[6]} ${tab[7]} ${tab[8]} ${tab[9]} "\n";
exit